{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "off",
    "class-methods-use-this": "off",
    "camelcase": "off",
    "no-param-reassign": "off",
    "import/no-extraneous-dependencies": "off"
  }
}